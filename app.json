{
  "expo": {
    "name": "astronomical-adventure",
    "slug": "astronomical-adventure",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#1C1C1C"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.uncled3v.astronomicaladventure",
      "googleServicesFile": "./GoogleService-Info.plist"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#1C1C1C"
      },
      "package": "com.uncled3v.astronomicaladventure",
      "googleServicesFile": "./GoogleService-Info.json"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/icon.png"
    },
    "plugins": [
      "expo-router",
      "@react-native-google-signin/google-signin",
      ["expo-build-properties", {
        "ios": {
          "useFrameworks": "static"
        }
      }],
      "@react-native-firebase/app",
      "@react-native-firebase/auth"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "2619f3a9-b0a2-477c-9e8c-bfd776444314"
      }
    }
  }
}
